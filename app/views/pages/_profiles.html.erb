<div class="col-md-4 col-md-offset-2 text-center">
  <div class="well">
    <% if current_user.profile %>
      <%= link_to 'View your profile', '#', class: 'btn btn-default btn-lg' %>
      <%= link_to 'Edit your profile', edit_profile_path(current_user.id), class: 'btn btn-default btn-lg' %> 
    <% else %>
      <%= link_to 'Create profile', new_profile_path, class: 'btn btn-primary btn-lg' %> 
    <% end %> 
  </div>
</div>
